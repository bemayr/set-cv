{
  "private": true,
  "name": "set-cv",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build --no-prerender",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch --https",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest ./tests"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "dependencies": {
    "@flatlinediver/react-spinner": "^2.0.10",
    "@react-css/flex": "^1.4.4",
    "@rmwc/fab": "^6.1.4",
    "@rmwc/icon": "^6.1.4",
    "@rmwc/list": "^6.1.4",
    "@rmwc/menu": "^6.1.4",
    "@rmwc/theme": "^6.1.4",
    "@xstate/react": "^1.3.3",
    "history": "^4.10.1",
    "opencv-ts": "^1.2.14",
    "preact": "^10.3.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-use-sizes": "^1.0.8",
    "react-webcam": "^5.2.3",
    "rooks": "^5.0.2",
    "xstate": "^4.19.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@xstate/inspect": "^0.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "jest": "^26.2.2",
    "jest-preset-preact": "^4.0.2",
    "preact-cli": "^3.0.0",
    "sirv-cli": "^1.0.0-next.3",
    "typescript": "^3.7.5"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.ts",
      "<rootDir>/tests/__mocks__/setupTests.ts"
    ]
  }
}
